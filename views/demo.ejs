    <title>Overcooked Demo</title>
    <link rel="Favicon" href="favicon.ico" />

    <script src="lib/jquery-min.js" type="text/javascript"> </script>
    <script src="lib/d3.v3.min.js" type="text/javascript"> </script>
    <script src="lib/babel.min.js" type="text/javascript"> </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>

    <script src="js/demo/demo.js" type="text/javascript"> </script>

    <link rel="stylesheet" href="css/bootstrap.min.css" type="text/css" />
    <link rel="stylesheet" href="css/style.css" type="text/css" />
  </head>
  <body>
    <form>
      <div class="container">
      <div class="row text-center">
	<div class="form-group col-lg-6">
	  <label for="agent">AI agent</label>
	  <select class="form-control" id="agent">
	    <option value="ppo_bc">Human-aware PPO agent</option>
	    <option value="ppo_sp">Self-play agent</option>
	    <option value="pbt">Population Based Training agent</option>
	  </select>
	</div>
	<div class="form-group col-lg-6">
	  <label for="layout">Layout</label>
	  <select class="form-control" id="layout">
	    <option value="cramped_room">Cramped Room</option>
	    <option value="asymmetric_advantages">Asymmetric Advantages</option>
	    <option value="coordination_ring">Coordination Ring</option>
	    <option value="random0">Forced Coordination</option>
	    <option value="random3">Counter Circuit</option>
	  </select>
	</div>
      </div>
      </div>
    </form>

    <div id="overcooked" class="text-center"></div>
    <div id="control" class="text-center"></div>

